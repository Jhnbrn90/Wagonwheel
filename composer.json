{
    "name": "sammyjo20/wagonwheel",
    "description": "Offer an online version of your Laravel emails to users.",
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sammyjo20",
            "email": "sam.carre2000@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/mail": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0",
        "illuminate/http": "^7.0 || ^8.0",
        "illuminate/routing": "^7.0 || ^8.0",
        "illuminate/console": "^7.0 || ^8.0",
        "paquettg/php-html-parser": "^3.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sammyjo20\\Wagonwheel\\WagonwheelServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Sammyjo20\\Wagonwheel\\": "src/"
        }
    }
}
